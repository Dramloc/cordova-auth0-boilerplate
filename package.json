{
  "name": "cordova-auth0-boilerplate",
  "version": "0.0.0",
  "description": "Cordova + Auth0 + Preact boilerplate",
  "keywords": [
    "preact",
    "cordova",
    "auth0",
    "boilerplate"
  ],
  "author": "Antoine Colmard <a.colmard@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "autoprefixer": "^8.4.1",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.1.5",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@auth0/cordova": "^0.3.0",
    "auth0-js": "^9.5.1",
    "classnames": "^2.2.5",
    "cordova-android": "^7.0.0",
    "cordova-plugin-customurlscheme": "^4.3.0",
    "cordova-plugin-safariviewcontroller": "^1.5.3",
    "cordova-plugin-whitelist": "^1.3.3",
    "es6-promisify": "^6.0.0",
    "history": "^4.7.2",
    "preact": "^8.2.9",
    "preact-router": "^2.6.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-safariviewcontroller": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "com.dramloc.cordova.test",
        "ANDROID_SCHEME": "com.dramloc.cordova.test",
        "ANDROID_HOST": "dramloc.eu.auth0.com",
        "ANDROID_PATHPREFIX": "/cordova/com.dramloc.cordova.test/callback"
      }
    },
    "platforms": [
      "android"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": "off",
      "react/prop-types": "off",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^h$"
        }
      ]
    }
  },
  "eslintIgnore": [
    "www",
    "platforms",
    "plugins"
  ]
}
